{
  "compilerOptions": {
	"target": "es2017",
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": [
      "ES2021",
      "dom"
    ],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "."
  ],
  "exclude": [
    "cypress/**/*"
  ]
}
